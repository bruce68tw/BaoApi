using Base.Models;
using Base.Services;

namespace BaoApi.Services
{
    public class BaoEdit : XpEdit
    {
        public BaoEdit(string ctrl) : base(ctrl) { }

        override public EditDto GetDto()
        {
            return new EditDto
            {
				Table = "dbo.[CustInput]",
                PkeyFid = "Id",
                Col4 = null,
                Items = new EitemDto[] 
				{
					new() { Fid = "Id" },
                    new() { Fid = "FldCheck", Required = true },
                    new() { Fid = "FldDate", Required = true },
                    new() { Fid = "FldDt", Required = true },
                    new() { Fid = "FldDec", Required = true },
                },
            };
        }

    } //class
}
